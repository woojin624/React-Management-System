{
  "name": "management",
  "version": "1.0.0",
  "script": {
    "client": "cd client && yarn start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-other-on-fail \"yarn server\" \"yarn client\""
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "nodemon": "^2.0.7"
  },
  "devDependencies": {
    "concurrently": "^4.0.1"
  }
}
